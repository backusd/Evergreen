{
	"button2_details": {
		"Margin": [ 5, 5, 5, 5 ],
		"Padding": [ 10, 5, 10, 5 ]
	},

	"row_max_min": {
		"MaxHeight": "1000",
		"import": "row_min.json"
	},

	"MenuBarLayoutPosition": {
		"Row": 0,
		"Column": 0,
		"ColumnSpan": 3
	},

	"CustomTextStyle": {
		"Type": "TextStyle",
		"Color": "Green",
		"FontFamily": "Arial",
		"FontSize": 24
	},

	"root": {

		"RowDefinitions": [
			{
				"Height": "50",
				"BottomAdjustable": true,
				"MinHeight": "40"
			},
			{
				"Height": "20%",
				"TopAdjustable": true,
				"import": "row_max_min"
			},
			{
				"Height": "1*"
			}
		],
		"ColumnDefinitions": [
			{
				"Width": "50%",
				"RightAdjustable": true,
				"MinWidth": "20%"
			},
			{
				"Width": "30",
				"LeftAdjustable": true
			},
			{ "Width": "2*" },
			{ "Width": "1*" }
		],

		"MenuBarLayout": {
			"Type": "Layout",
			"import": "MenuBarLayoutPosition",
			"RowDefinitions": [ { "Height": "1*" } ],
			"ColumnDefinitions": [
				{ "Width": 50 },
				{ "Width": 50 },
				{ "Width": 50 },
				{ "Width": "1*" }
			],

			"FileText": {
				"Type": "Text",
				"Text": "File",
				"Style": "CustomTextStyle"
			},

			"EditText": {
				"Type": "Text",
				"Text": "Edit",
				"Column": 1,
				"Style": "CustomTextStyle"
			},

			"ViewText": {
				"Type": "Text",
				"Text": "View",
				"Column": 2,
				"Color": "Blue",
				"FontFamily": "Arial",
				"FontSize": 16,
				"Badkey":  1
			}



		},


		"TestButton": {
			"Type": "Button",
			"Content": "This is a test",
			"Row": 0,
			"Column": 0,
			"import": "button_details.json",
			"OnClick": "TestButtonOnClick"
		},

		"TestButton2": {
			"Type": "Button",
			"Content": "This is a second test",
			"Row": 0,
			"Column": 1,
			"ColumnSpan": 2,
			"import": "button2_details",
			"OnClick": "TestButton2OnClick"
		}
	}
}